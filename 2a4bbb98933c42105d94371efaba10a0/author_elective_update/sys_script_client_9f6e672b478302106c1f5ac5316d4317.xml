        <messages/>
        <name>Set Assignment Group</name>
        <order/>
        <script><![CDATA[function onLoad() {
   //Get the Category value, pass to switch and set the assignment group.

   var category = g_form.getValue('category');
   var assignmentGroup = g_form.getValue('assignment_group');

   if(!assignmentGroup){

    switch (category){
     case 'Other':
	 g_form.setValue('assignment_group','Managers')
	 break;

	 case 'Vehicles':
	 g_form.setValue('assignment_group','Maintenance')
	 break;



	}




	
   }else{
	alert("===2" + assignmentGroup + category)
   }


}]]></script>
